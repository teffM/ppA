	<queryString language="SQL">
		<![CDATA[SELECT
     clientes."id" AS clientes_id,
     clientes."nombre" AS clientes_nombre,
     clientes."apellido" AS clientes_apellido,
     clientes."telefono" AS clientes_telefono,
     clientes."correo" AS clientes_correo,
     clientes."dui" AS clientes_dui,
     clientes."nit" AS clientes_nit,
     clientes."comprobanteIva" AS clientes_comprobanteIva,
     clientes."descripcion" AS clientes_descripcion,
     reservaciones."id" AS reservaciones_id,
     reservaciones."idSucursal" AS reservaciones_idSucursal,
     reservaciones."idEstado" AS reservaciones_idEstado,
     reservaciones."idCliente" AS reservaciones_idCliente,
     reservaciones."idUsuario" AS reservaciones_idUsuario,
     reservaciones."fechaReservacion" AS reservaciones_fechaReservacion,
     reservaciones."numPersonas" AS reservaciones_numPersonas,
     reservaciones."comentarios" AS reservaciones_comentarios,
     reservaciones."fechaCreacion" AS reservaciones_fechaCreacion
FROM
     "dbo"."clientes" clientes INNER JOIN "dbo"."reservaciones" reservaciones ON clientes."id" = reservaciones."idCliente"]]>
	</queryString>
